         ("<=" . 'font-lock-constant-face)
         (">=" . 'font-lock-constant-face)
         ("#"  . 'font-lock-constant-face)
         ("\\<in\\>" . 'font-lock-constant-face)

                                      ;; common attributes
         ("\\<head\\>"      . 'font-lock-builtin-face)
         ("\\<tail\\>"      . 'font-lock-builtin-face)
