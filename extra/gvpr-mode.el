                                      ;; built-in variables
         ("\\<$\\>"        . 'font-lock-builtin-face)
         ("\\<$F\\>"       . 'font-lock-builtin-face)
         ("\\<$G\\>"       . 'font-lock-builtin-face)
         ("\\<$NG\\>"      . 'font-lock-builtin-face)
