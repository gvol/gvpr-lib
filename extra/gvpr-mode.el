                                      ;; clauses
         ("\\<BEGIN\\>"   . 'font-lock-preprocessor-face)
         ("\\<BEGIN_G\\>" . 'font-lock-preprocessor-face)
         ("\\<N\\>"       . 'font-lock-preprocessor-face)
